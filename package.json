{
  "name": "gql-server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev src/server.ts",
    "serve": "ts-node src/server.ts",
    "watch": "tsc --project tsconfig.json --watch",
    "prebuild": "rimraf build",
    "build": "tsc --project tsconfig.build.json",
    "prestart": "npm run build",
    "start": "cross-env NODE_ENV=production node build/src/server.js",
    "lint": "eslint . --ext .ts ./src --fix"
  },
  "size-limit": [
    {
      "path": "build/",
      "limit": "240 MB"
    }
  ],
  "dependencies": {
    "@envelop/core": "^3.0.3",
    "@envelop/execute-subscription-event": "^3.0.3",
    "@envelop/response-cache": "^4.0.3",
    "@envelop/response-cache-redis": "^2.0.3",
    "@typegoose/typegoose": "^10.3.0",
    "apollo-server-express": "^3.4.0",
    "apollo-server-plugin-base": "^3.6.2",
    "bcrypt": "^5.0.1",
    "bson": "^5.0.1",
    "class-transformer": "0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.17.1",
    "formidable": "^2.0.1",
    "graphql": "^15.3.0",
    "graphql-query-complexity": "^0.12.0",
    "ioredis": "^5.2.3",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.1",
    "mongoose-autopopulate": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "shortid": "^2.2.16",
    "type-graphql": "^1.1.1",
    "typedi": "^0.10.0"
  },
  "devDependencies": {
    "@size-limit/preset-app": "^8.2.4",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie": "^0.5.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/graphql": "^14.5.0",
    "@types/shortid": "^0.0.29",
    "cross-env": "^7.0.3",
    "size-limit": "^8.2.4",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.4"
  }
}
