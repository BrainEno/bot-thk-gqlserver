{
  "name": "gql-server",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon --watch 'src/**/*.ts' --exec ts-node src/server.ts",
    "watch": "tsc --project tsconfig.json --watch",
    "build": "tsc --project tsconfig.build.json",
    "prestart": "npm run build",
    "start": "cross-env NODE_ENV=production node --experimental-specifier-resolution=node --loader ts-node/esm build/server.js",
    "lint": "eslint . --ext .ts ./src --fix"
  },
  "dependencies": {
    "@apollo/utils.keyvaluecache": "^2.1.0",
    "@sendgrid/mail": "^7.7.0",
    "@typegoose/typegoose": "^11.0.2",
    "apollo-server-express": "^3.12.0",
    "apollo-server-plugin-base": "^3.6.2",
    "bcrypt": "^5.0.1",
    "bson": "^5.0.1",
    "class-transformer": "0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.17.1",
    "graphql": "^15.3.0",
    "graphql-query-complexity": "^0.12.0",
    "graphql-redis-subscriptions": "^2.6.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-ws": "^5.14.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.1",
    "mongoose-autopopulate": "^1.0.0",
    "pino": "^8.14.1",
    "pino-pretty": "^10.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.0",
    "shortid": "^2.2.16",
    "type-graphql": "^1.1.1",
    "typedi": "^0.10.0",
    "ws": "^8.13.0"
